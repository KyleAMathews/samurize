  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyleAMathews/vite-react-router-electric-sql-starter.git"
  },
  "scripts": {
    "backend:start": "docker compose --env-file src/backend/compose/.envrc -f src/backend/compose/docker-compose.yaml up",
    "backend:prod:start": "docker compose --env-file src/backend/compose/.envrc -f src/backend/compose/docker-compose-prod.yaml up --detach",
    "backend:down": "docker compose  --env-file src/backend/compose/.envrc -f src/backend/compose/docker-compose.yaml down --volumes",
    "build": "cross-env-shell ELECTRIC_URL=$ELECTRIC_URL ELECTRIC_PROXY=$ELECTRIC_PROXY \"npm run client:generate:prod && vite build\"",
    "client:generate": "npx electric-sql generate --service http://localhost:5133",
    "client:generate:prod": "npx electric-sql generate --service $ELECTRIC_URL --proxy $ELECTRIC_PROXY",
    "db:migrate": "node ./db/migrate.cjs",
    "db:psql": "node ./db/connect.cjs",
    "cleanup-example-code": "node scripts/cleanup-example-code.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npx tsx watch src/backend/index.ts\" \"vite\"",
    "partykit:deploy": "partykit deploy"
  },
  "devDependencies": {
    "@simbathesailor/use-what-changed": "^2.0.0",
    "partykit": "0.0.45",
    "vitest": "^1.0.4"
  }
}

{
  "name": "vite-react-router-electric-sql-starter",
  "description": "Somewhat opinionated starter for ElectricSQL with Vite, React Router, and trpc-electric-sql",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/KyleAMathews/vite-react-router-electric-sql-starter/issues"
  },
  "dependencies": {
    "@databases/pg-migrations": "^5.0.2",
    "@electric-sql/prisma-generator": "1.1.0",
    "@prisma/client": "4.8.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "better-sqlite3": "^8.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electric-sql": "^0.7.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "pg": "^8.11.3",
    "prettier": "^3.1.0",
    "prisma": "4.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "shelljs": "^0.8.5",
    "trpc-electric-sql": "^0.1.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "wa-sqlite": "rhashimoto/wa-sqlite#master"
  },
  "homepage": "https://github.com/KyleAMathews/vite-react-router-electric-sql-starter#readme",
  "keywords": [
    "electric-sql",
    "javascript",
    "react",
    "react-router",
    "starter",
    "typescript",
    "vite"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyleAMathews/vite-react-router-electric-sql-starter.git"
  },
  "scripts": {
    "backend:start": "docker compose --env-file src/backend/compose/.envrc -f src/backend/compose/docker-compose.yaml up",
    "backend:down": "docker compose -f backend/compose/docker-compose.yaml down --volumes",
    "client:generate": "npx electric-sql generate --service http://localhost:5133",
    "db:migrate": "node ./db/migrate.cjs",
    "db:psql": "node ./db/connect.cjs",
    "cleanup-example-code": "node scripts/cleanup-example-code.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npx tsx watch src/backend/index.ts\" \"vite\""
  }
}
